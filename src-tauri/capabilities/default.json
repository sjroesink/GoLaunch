{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Default capabilities for GoLaunch",
  "windows": ["main"],
  "permissions": [
    "core:default",
    "core:event:default",
    "shell:allow-open",
    "global-shortcut:allow-register",
    "global-shortcut:allow-unregister",
    "shell:allow-spawn",
    "shell:allow-execute"
  ]
}
